{
  "manifestVersion": 1,
  "id": "nuget-authenticator",
  "name": "NuGet Authenticator",
  "version": "0.1.5",
  "publisher": "athomas",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Provides an easy way to authenticate to an external NuGet feed.",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "key-128.png"
  },
  "tags": ["NuGet", "credentials", "authenticate"],
  "screenshots": [{
    "path": "screenshots/NuGetSetCredentials.png"
  }],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "license.md"
    }
  },
  "files": [
    {
      "path": "Tasks/NuGetSetCredentials"
    }
  ],
  "contributions": [
    {
      "id": "nuget-set-credentials",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/NuGetSetCredentials"
      }
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adriaanthomas/NuGetAuthenticator"
  }
}
